<h1>Attraction Categories</h1>

<ul>
	<% @categories.each do |category| %>
		<li><h3><%= link_to category.name, category_path(category) %></h3></li>
	<% end %>
</ul>

<br />		

<% if current_user.guide_status %>
	<%= link_to "Add a New Category", new_category_path, class: "btn btn-primary" %>
<% end %>